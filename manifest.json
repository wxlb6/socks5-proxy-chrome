{
    "manifest_version": 3,
    "name": "Socks5 Proxy For Chrome",
    "version": "1.2",
    "description": "Socks5 proxy for Google Chrome.",
    "permissions": [
        "proxy",
        "storage",
        "unlimitedStorage",
        "downloads"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon-off.png",
            "48": "icons/icon-off.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon-off.png",
        "48": "icons/icon-off.png",
        "128": "icons/icon128.png"
    }
}