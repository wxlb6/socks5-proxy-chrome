{
  "manifest_version": 3,
  "name": "Socks5 Proxy For Chrome",
  "version": "1.0",
  "description": "Socks5 proxy for Google Chrome",
  "permissions": [
    "proxy",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-off.png",
      "48": "icons/icon-off.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-off.png",
    "48": "icons/icon-off.png",
    "128": "icons/icon128.png"
  }
}